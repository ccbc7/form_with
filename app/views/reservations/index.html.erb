<h1>一覧画面</h1>

<table>
  <thead>
    <tr>
      <th>id</th>
      <th>ルームネーム</th>
      <th>開始日</th>
    </tr>
  </thead>

  <tbody>
    <% @reservations.each do |r| %>
      <tr>
        <td>
          <%= r.id %>
        </td>

        <td>
          <%= r.room_name %>
        </td>

        <td>
          <%= r.start_day %>
        </td>
      </tr>
      <% end %>
  </tbody>
</table>

<p>
  <%= link_to "新規登録" , "reservations/new" %>
</p>
